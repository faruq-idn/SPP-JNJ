{
  "version": 2,
  "framework": null,
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.1",
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "buildCommand": "chmod +x vercel-install.sh && ./vercel-install.sh",
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/test",
      "dest": "/api/test.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "development",
    "APP_DEBUG": "true",
    "VERCEL_FORCE_NO_BUILD_CACHE": "1",
    "VERCEL_DEBUG_BUILD": "1",
    "VERCEL_DEBUG": "1"
  }
}
