{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "api/*.php",
      "use": "vercel-php@0.7.1"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/test",
      "dest": "/api/test.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_NAME": "SPP-JNJ",
    "APP_ENV": "local",
    "APP_DEBUG": "true",
    "APP_KEY": "base64:5CpEbqPQx/+FinF+ztkQzfAQHjvzYDp03+49/6+8V30=",
    "LOG_CHANNEL": "stderr",
    "LOG_LEVEL": "debug",
    "CACHE_DRIVER": "array",
    "SESSION_DRIVER": "cookie",
    "VIEW_COMPILED_PATH": "/tmp/views"
  }
}
