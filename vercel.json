{
  "version": 2,
  "framework": null,
  "buildCommand": "node vercel-pre-build.js",
  "builds": [
    {
      "src": "api/*.php",
      "use": "vercel-php@0.7.1",
      "config": {
        "maxDuration": 60,
        "memory": 1024
      }
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/robots.txt",
      "dest": "/public/robots.txt"
    },
    {
      "src": "/test",
      "dest": "/api/test.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "development",
    "APP_DEBUG": "true",
    "CACHE_DRIVER": "array",
    "SESSION_DRIVER": "cookie",
    "VIEW_COMPILED_PATH": "/tmp/views",
    "CACHE_PATH": "/tmp/cache",
    "SESSION_PATH": "/tmp/sessions",
    "BOOTSTRAP_CACHE_PATH": "/tmp/bootstrap/cache",
    "LOG_CHANNEL": "stderr",
    "FILESYSTEM_DRIVER": "null",
    "NODE_ENV": "production"
  }
}
